{
    "name": "Salesforce OmniLogin",
    "version": "0.4.1",
    "description": "Open Source OAuth2 Store for Salesforce",
    "permissions": [
      "storage", 
      "identity",
      "https://login.salesforce.com/services/oauth2/*",
      "https://test.salesforce.com/services/oauth2/*",
      "https://*.my.salesforce.com/services/oauth2/*"
    ],
    "icons": {
      "32": "icons/32.png",
      "128": "icons/128.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Salesforce OmniLogin",
      "default_popup": "index.html"
    },
    "manifest_version": 2
  }